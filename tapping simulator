local DiscordLib = loadstring(game:HttpGet"https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/discord%20lib.txt")()

local win = DiscordLib:Window("Larryhun")

local serv = win:Server("Tapping Simulator", "")

local btns = serv:Channel("Teleports")

btns:Button("Kill all", function()
DiscordLib:Notification("Notification", "Killed everyone!", "Okay!")
end)

btns:Seperator()

btns:Button("Get max level", function()
DiscordLib:Notification("Notification", "Max level!", "Okay!")
end)

local tgls = serv:Channel("Auto")

getgenv().autoTap = true;
getgenv().autoRebirth = true;
getgenv().autoBigTap = true;

tgls:Toggle("Auto-Tap",false, function(bool)
DiscordLib:Notification("Notification", "Auto tap is Enabled!", "Okay!")
getgenv().autoTap = bool
    print("Auto tap is", bool)
    if bool then
        doTap();
    end
end)

tgls:Toggle("Auto-Big-Tap",false, function(bool)
DiscordLib:Notification("Notification", "Auto big tap is Enabled!", "Okay!")
getgenv().autoTap = bool
    print("Auto big tap is", bool)
    if bool then
        doBigTap();
    end
end)

tgls:Toggle("Auto-Rebirth",false, function(bool)
DiscordLib:Notification("Notification", "Auto rebirth is Enabled!", "Okay!")
getgenv().autoRebirth = bool
    print("Auto rebirth is", bool)
    if bool then
        doRebirth();
    end
end)

local pet = serv:Channel("Pet")

getgenv().autoOpenPet = true;

pet:Toggle("Auto-Open-Pet",false, function(bool)
DiscordLib:Notification("Notification", "Auto open pet is Enabled!", "Okay!")
getgenv().autoOpenPet = bool
    print("Auto open pet is", bool)
    if bool then
        doPet();
    end
end)


function doTap()
    spawn(function()
        while getgenv().autoTap == true do
            local args = {[1] = false,}
            game:GetService("ReplicatedStorage").PlayerClick:FireServer(unpack(args))
            wait(0.5)
        end
    end)
end

function doBigTap()
	spawn(function()
		while getgenv().autoBigTap == true do
			game:GetService("ReplicatedStorage").TapSplosion:InvokeServer()
			wait()
		end
	end)
end

function doRebirth()
    spawn(function()
        while getgenv().autoRebirth == true do
            local args = {[1] = 50,}
            game:GetService("ReplicatedStorage").PlayerRebirth:FireServer(unpack(args))
            wait()
        end
    end)
end

function doPet()
    spawn(function()
        while getgenv().autoOpenPet == true do
            local args = {[1] = "StandardShop",[2] = 3,}
            game:GetService("ReplicatedStorage").PetStoreRoll:InvokeServer(unpack(args))
            wait()
        end
    end)
end

win:Server("Main", "http://www.roblox.com/asset/?id=6031075938")
